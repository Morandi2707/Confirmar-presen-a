{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "google-sheets-api/api/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "google-sheets-api/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/$1"
      }
    ]
  }
  